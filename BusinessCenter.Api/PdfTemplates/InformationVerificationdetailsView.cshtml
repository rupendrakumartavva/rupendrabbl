<!DOCTYPE html>
<meta http-equiv="content-type" content="text/html; charset=utf-8" />
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1" />
@using System.Web.Mvc.Html
@using System;
@using System.Globalization

@model BusinessCenter.Data.Model.SubmissionVerfication

@{
    Layout = null;
}

<html>
<head>
    <title>title</title>

    <link rel="stylesheet" type="text/css" href="@System.Configuration.ConfigurationManager.AppSettings["siteAddress"]/css/app.css" />
    <link rel="stylesheet" type="text/css" href="@System.Configuration.ConfigurationManager.AppSettings["siteAddress"]/css/normalize.css">
    <link rel="stylesheet" type="text/css" href="@System.Configuration.ConfigurationManager.AppSettings["siteAddress"]/css/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="@System.Configuration.ConfigurationManager.AppSettings["siteAddress"]/css/style.css">
    <link rel="stylesheet" type="text/css" href="@System.Configuration.ConfigurationManager.AppSettings["siteAddress"]/css/temp.css">
    <link rel="stylesheet" type="text/css" href="@System.Configuration.ConfigurationManager.AppSettings["siteAddress"]/less/bbl.css">
    <link rel="stylesheet" type="text/css" href="@System.Configuration.ConfigurationManager.AppSettings["siteAddress"]/css/flexslider.css" />
    <link rel="stylesheet" type="text/css" href="@System.Configuration.ConfigurationManager.AppSettings["siteAddress"]/css/fileinput.css">
    <link rel="stylesheet" type="text/css" href="@System.Configuration.ConfigurationManager.AppSettings["siteAddress"]/css/customstyle.css" />
    <link href="@System.Configuration.ConfigurationManager.AppSettings["siteAddress"]/css/bootstrap-datetimepicker.min.css" rel="stylesheet" type="text/css" />
    <link href="@System.Configuration.ConfigurationManager.AppSettings["siteAddress"]/css/datetimepicker.css" rel="stylesheet" type="text/css" />
</head>
<body>

    <div class="page_title">
        <div class="container">
            <div class="row">
                <h2 tabindex="0">Information Verification</h2>
                <span class="font_how_to_use">How To Use This Page <span class="glyphicon glyphicon-chevron-down"></span></span>
                <div class="clearfix"></div>
                <p style="margin-top:8px">
                    Please review your application. Based on the information you have provided, your Basic Business License will include the following Business License Categories and Fees. Please carefully review the information presented below before selecting the "Next"   If you notice any errors in the license categories, you must select the "Return to Checklist" button to make changes.
                </p>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="row">
            <div class="timeLineWrapper clearfix">
                <div class="progress_steps">
                    <div class="timeLine"></div>
                    <a class="active" href="">
                        <div class="timeline_list step1">
                            <span class="inCircle activeCir"></span>
                            <div class="step_detail">
                                <p class="step_no active">
                                    <strong>Step 1</strong>
                                </p>
                                <p class="step_no">
                                    <strong>Pre-Application Questions</strong>
                                </p>
                                <p class="step_text active"></p>
                            </div>
                        </div>
                    </a>
                    <a class="active" tabindex="-1">
                        <div class="timeline_list step2">
                            <span class="inCircle activeCir"></span>
                            <div class="step_detail">
                                <p class="step_no active">
                                    <strong>Step 2</strong>
                                </p>
                                <p class="step_no">
                                    <strong>Application Checklist</strong>
                                </p>
                                <p class="step_text active"></p>
                            </div>
                        </div>
                    </a>
                    <a class="" tabindex="-1">
                        <div class="timeline_list step3">
                            <span class="inCircle activeCir"></span>
                            <div class="step_detail">
                                <p class="step_no active">
                                    <strong>Step 3</strong>
                                </p>
                                <p class="step_no">
                                    <strong>Information Verification</strong>
                                </p>
                                <p class="step_text active"></p>
                            </div>
                        </div>
                    </a>
                    <a class="" tabindex="-1">
                        <div class="timeline_list step4">
                            <span class="inCircle "></span>
                            <div class="step_detail">
                                <p class="step_no ">Step 4</p>
                                <p class="step_no">Payment</p>
                                <p class="step_text "></p>
                            </div>
                        </div>
                    </a>
                    <a class="" tabindex="-1">
                        <div class="timeline_list step5">
                            <span class="inCircle "></span>
                            <div class="step_detail">
                                <p class="step_no ">Step 5</p>
                                <p class="step_no">Receipt and Next Steps</p>
                                <p class="step_text "></p>
                            </div>
                        </div>
                    </a>
                </div>
            </div>
        </div>
    </div>
    <div id="dvMainsection">
        <div class="wizard_form">
            <div class="container">
                <div class="row">
                    <div class="result_top_block" tabindex="0">
                    </div>
                </div>

                <div id="infoverify">
                    <div class="collapse_panels" id="collapse_panels">

                        <div class="panel-group" id="checkListAccordion" role="tablist" aria-multiselectable="true">

                            <div class="panel panel-default">
                                <div class="panel-heading row" role="tab" id="heading1">
                                    <div class="panel-title">
                                        <div class="checkListTostart">
                                            <div class="col-lg-11 col-md-11 col-sm-11 col-xs-11 bot_border" tabindex="0">
                                                <h3>Business Activities</h3>
                                            </div>
                                            <div class="col-lg-1 col-md-1 col-sm-1 col-xs-1 collapse_button bot_border">
                                                <div toggle-icon tabindex="0" aria-label="Press return key to toggle Collapsible Panel" class="collapseIcon hide-details" data-toggle="collapse" data-target="#collapse1"></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div id="collapse1" class="panel-collapse collapse in" aria-labelledby="heading1">
                                    <div class="business-activities-wrapper">
                                        <div class="row">
                                            <div class="col-md-16 col-sm-16 col-xl-16 col-xs-16 col-md-offset-4 col-xs-offset-4 col-sm-offset-4">

                                                <div class="panel text-center grand_total" style="width:100%!important">
                                                    <div class="panel-body">
                                                        @*<h1>@string.Format(new CultureInfo("en-US"), "{0:C}", @Model.GrandTotal) </h1>*@
                                                        <h1 style="text-align: center; margin-top: 4px;" id="grandamount">$ @Model.GrandTotals</h1>
                                                    </div>
                                                    <div class="panel-footer">
                                                        <span>
                                                            Grand <br />Total
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="panel-body">
                                                <div class="search_result adjust_font">
                                                    @foreach (var item in Model.ServiceCheckList)
                                                    {
                                                        <section class="clearfix">
                                                            <div class="preappgrid container">
                                                                <div class="innerRow" style="margin-top: 10px;">

                                                                    <div class="col-xs-1 col-sm-1  arrow no-pad" style="float: left;">
                                                                        <span class="arrow-right hide-details" ng-click="vm.expandCollapse($event)"></span>
                                                                        @*<span class="arrow-bottom" style="background: url('@System.Configuration.ConfigurationManager.AppSettings["siteAddress"]/images/arrow-bottom.png') 0 center no-repeat"></span>*@
                                                                    </div>

                                                                    <div class="col-xs-3 col-sm-3  no-pad bbl" style="float: left">

                                                                        <p>
                                                                            <span>Endorsement </span>
                                                                        </p>

                                                                        <div>
                                                                            <strong class="font_sze">@item.Endorsement </strong>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-xs-5 col-sm-5  no-pad" style="float: left">
                                                                        <p>
                                                                            <span>Category ID : </span>@item.CategoryCode
                                                                        </p>
                                                                        <p>
                                                                            <span>License Category : </span>
                                                                            @item.LicenseCategory
                                                                        </p>
                                                                        @if (item.SubCategoryName != "")
                                                                        {
                                                                            <p>
                                                                                <span>Sub-Category : </span>@item.SubCategoryName
                                                                            </p>
                                                                        }
                                                                    </div>

                                                                    <div class="col-xs-3 col-sm-3 no-pad" style="float: left">
                                                                        <p>
                                                                            <span># Units : </span>
                                                                            @item.Units
                                                                        </p>
                                                                        <p>
                                                                            <span>License Duration : </span>
                                                                            @if (item.LicenseDuration.ToUpper().Trim() == "FOUR (4) YEAR")
                                                                            {

                                                                                <span>4 Year</span>
                                                                            }
                                                                            else
                                                                            {
                                                                                <span>2 Year</span>
                                                                            }
                                                                        </p>
                                                                        <p>
                                                                            <span>Total Fee : </span>
                                                                            <strong>@string.Format(new CultureInfo("en-US"), "{0:C}", @item.TotalFee)</strong>
                                                                        </p>
                                                                    </div>
                                                                </div>
                                                                <div class="details no-pad" style="margin-top: 20px; display: block">
                                                                    <div class="topBorder col-xs-11 col-sm-11 col-md-11 col-lg-11 no-pad col-md-offset-1"></div>
                                                                    <div class="col-xs-10 col-sm-10 col-md-10 col-lg-10 no-pad col-md-offset-2 col-xs-offset-2 col-sm-offset-2">
                                                                        <div class="col-xs-10 col-sm-10 col-md-10 col-lg-10 no-pad text-center">
                                                                            <p style="padding-left:150px;">
                                                                                <span>Application Fee  : </span>@string.Format(new CultureInfo("en-US"), "{0:C}", @item.ApplicationFee)
                                                                            </p>
                                                                            <p>
                                                                                <span>Endorsement Fee : </span>@string.Format(new CultureInfo("en-US"), "{0:C}", @item.EndorsementFee)
                                                                            </p>
                                                                            <p>
                                                                                <span>Category License Fee  : </span>@string.Format(new CultureInfo("en-US"), "{0:C}", @item.CategoryLicenseFee)
                                                                            </p>

                                                                            <div class="topBorder col-md-offset-2 col-xs-offset-2 col-sm-offset-2"></div>
                                                                            <p>
                                                                                <span>Sub Total : </span>@string.Format(new CultureInfo("en-US"), "{0:C}", @item.SubTotal)
                                                                            </p>
                                                                            <p class="hidden-sm">
                                                                                <span>10% Tech Fee  : </span>@string.Format(new CultureInfo("en-US"), "{0:C}", @item.TechFee)
                                                                            </p>
                                                                            @if (item.LicenseCategory == "Apartment" || item.LicenseCategory == "One Family Rental")
                                                                            {

                                                                                <div class="col-sm-12 text-center no-pad">
                                                                                    <p style="font-size: 14px;">
                                                                                        <strong> Category License Fee includes an RAO fee of $43.00/unit.</strong>
                                                                                    </p>
                                                                                </div>
                                                                            }
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </section>

                                                    }
                                                    @if (Model.IsSubmissioneHop == true)
                                                    {
                                                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 text-center no-pad">
                                                            <p style="font-size: 14px; font-style: italic; color: #6E6B6B; font-weight: 600; margin-top: 5px;">
                                                                <strong>** An eHOP Fee of $72.60 ($66.00 (eHOP Fee)+ 10% Technology Fee) was added to the Grand Total displayed above.</strong>
                                                            </p>
                                                        </div>
                                                    }
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="panel panel-default">
                                    <div class="panel-heading row" role="tab" id="heading2">
                                        <div class="panel-title">
                                            <div class="checkListTostart">
                                                <div class="col-lg-111 col-md-11 col-sm-11 col-xs-11 bot_border" tabindex="0">
                                                    <h3> Business Information</h3>
                                                </div>
                                                <div class="col-lg-1 col-md-1 col-sm-1 col-xs-1 collapse_button bot_border">
                                                    <div toggle-icon tabindex="0" aria-label="Press return key to toggle Collapsible Panel" class="collapseIcon hide-details" data-toggle="collapse" data-target="#collapse2"></div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div id="collapse2" class="panel-collapse open in" aria-labelledby="heading2"
                                         style="overflow: hidden; display: block;">

                                        <div tabindex="-1">
                                            <div class="row col-md-offset-1 col-sm-offset-1 col-xs-offset-1">
                                                <div class="col-sm-6 col-md-6  col-xs-6 panel-collapse-business-wrapper">

                                                    @if (@Model.IsSubmissionHop && @Model.OccupanyNumber != "")
                                                    {
                                                        <p><span>Home&nbsp;Occupancy&nbsp;Permit&nbsp;Number : </span> @Model.OccupanyNumber</p>
                                                    }

                                                    @if (@Model.IsSubmissioneHop && @Model.OccupanyNumber != "")
                                                    {
                                                        <p><span>Expedited&nbsp;Home&nbsp;Occupancy&nbsp;Permit&nbsp;Number : </span> @Model.OccupanyNumber</p>
                                                    }

                                                    @if (@Model.OccupanyNumber != "" && @Model.IsSubmissionCofo)
                                                    {
                                                        <p><span>Certificate&nbsp;of&nbsp;Occupancy&nbsp;Number : </span> @Model.OccupanyNumber</p>
                                                    }
                                                    <p>
                                                        <span>Corporate Registration File Number : </span>
                                                        @Model.CorpFileNo
                                                    </p>

                                                    <p>
                                                        <span>Organization Type : </span>
                                                        @Model.OrgType
                                                    </p>

                                                    @if (@Model.TradeName != "")
                                                    {
                                                        <p>
                                                            <span>Trade Name (if applicable) :  </span>
                                                            @Model.TradeName
                                                        </p>
                                                    }

                                                    @if (@Model.TradeName == "")
                                                    {
                                                        <p>
                                                            <span>Trade Name (if applicable) :  </span>
                                                            N/A
                                                        </p>
                                                    }
                                                </div>

                                                <div class="col-sm-6 col-md-6 col-xs-6 panel-collapse-business-wrapper">

                                                    @if (@Model.DateofIssue != "01/01/1900" && @Model.IsSubmissioneHop != true)
                                                    {
                                                        <p><span>Date of Issuance :  </span> @Model.DateofIssue</p>
                                                    }

                                                    @if (@Model.IsFEIN == false)
                                                    {
                                                        <p>
                                                            <span>Social Security Number (SSN) : </span>
                                                            @Model.FienNumber
                                                        </p>
                                                    }

                                                    @if (@Model.IsFEIN)
                                                    {
                                                        <p>
                                                            <span>Federal Employer Identification Number (FEIN) : </span>
                                                            @Model.FienNumber
                                                        </p>
                                                    }

                                                    <p>
                                                        <span>Business Name : </span>
                                                        @Model.HeadQBName
                                                    </p>
                                                </div>
                                            </div>
                                        </div>
                                        <div style="margin: 2%">
                                            <div class="line"></div>
                                        </div>
                                        <div style="clear: both;"></div>
                                        <div class="col-sm-3 col-xl-4 col-xs-3 business-address-panel-wrapper clearfix">
                                            <div class="panel panel-default collapse_panels panel_height" style="height:32em !important;">
                                                <div class="panel-heading">
                                                    <p style="text-align: center;">
                                                        Business HQ <br /> Address
                                                    </p>
                                                </div>
                                                <div class="panel-body">
                                                    @if (@Model.HeadQBName != "")
                                                    {
                                                        <p style="text-align: left;">@Model.HeadQBName </p>
                                                    }

                                                    @if (@Model.HeadQName != "")
                                                    {
                                                        <p style="text-align: left;">@Model.HeadQName</p>
                                                    }

                                                    @if (@Model.HeadQAddressNumber != "")
                                                    {
                                                        <p>@Model.HeadQAddressNumber</p>
                                                    }

                                                    @if (@Model.HeadQStreetName != "")
                                                    {
                                                        <p>@Model.HeadQStreetName</p>
                                                    }

                                                    @if (@Model.HeadQStreetType != "")
                                                    {
                                                        <p>@Model.HeadQStreetType </p>
                                                    }

                                                    @if (@Model.HeadQAQuadrant != "")
                                                    {
                                                        <p>@Model.HeadQAQuadrant</p>
                                                    }

                                                    @if (@Model.HeadQCity != "")
                                                    {<p>@Model.HeadQCity</p>
                                                    }

                                                    @if (@Model.HeadQState != "")
