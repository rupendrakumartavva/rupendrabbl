                                                                    <span>Document Required</span>
                                                                </p>

                                                                <div>
                                                                    @item.DocRequired
                                                                </div>
                                                            </div>
                                                            <div class="col-sm-1 col-xs-1"></div>
                                                            <div class="col-sm-4 col-xs-4 no-pad ">

                                                                <p>
                                                                    <span>Agency : </span>
                                                                    @item.Agency
                                                                </p>

                                                                <p>
                                                                    <span> Division : </span>@item.Division
                                                                </p>
                                                            </div>

                                                            <div class="col-sm-3 col-xs-3 no-pad">
                                                                <p>
                                                                    <span>License Code : </span>
                                                                    @item.CategoryCode
                                                                </p>
                                                                <p>
                                                                    <span>Endorsement : </span>
                                                                    @item.Endorsement
                                                                </p>
                                                                <p>
                                                                    <span> License : </span>@item.License
                                                                </p>
                                                            </div>
                                                            <div class="clearfix">
                                                                <div class="col-md-offset-1 col-xs-offset-1 col-sm-offset-1 col-md-11 col-xs-11 col-sm-11 no-pad">
                                                                    @if (@item.UploadFileName != "")
                                                                    {
                                                                        <p>
                                                                            <span>File Name :</span> @item.UploadFileName
                                                                        </p>
                                                                    }
                                                                </div>
                                                            </div>
                                                            <div class="col-sm-12 col-xs-12 col-md-12">
                                                                <div class="grid-row-line"></div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </section>
                                            }
