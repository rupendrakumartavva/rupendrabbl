                                                </div>
                                            </div>
                                            @*<div class="checkListTostart">
                                                    <div class="col-lg-8 col-md-7 col-sm-6 col-xs-12 bot_border" tabindex="0">
                                                        <h3>Supporting Documents </h3>
                                                    </div>
                                                    <div class="col-lg-3 col-md-4 col-sm-5 col-xs-12 btn_cont bot_border"></div>
                                                    <div class="col-lg-1 col-md-1 col-sm-1 col-xs-12 collapse_button">
                                                        <div toggle-icon tabindex="0" aria-label="Press return key to toggle Collapsible Panel" class="collapseIcon" data-toggle="collapse" data-target="#collapse3"></div>
                                                    </div>
                                                </div>*@
                                        </div>
                                    </div>
                                    <div id="collapse3" class="panel-collapse collapse in" aria-labelledby="heading3"
