<script>
    var text = $('#grandamount').text();
    var charsLength = text.length;

    if (charsLength > 9 && charsLength <= 12) {
        $(".grand_total").css("width", "130%");
    }
    else if (charsLength > 12) {
        $(".grand_total").css("width", "160%");
    }
</script>