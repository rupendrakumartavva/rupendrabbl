@using BusinessCenter.Admin.Helpers
@using BusinessCenter.Admin.Common;
@using System.Globalization;
@model BusinessCenter.Data.Model.ReceiptModel
@{
    ViewBag.Title = "DCRA - View Receipt";
}
@if (Session["UserName"] != null)
{
    Layout = "~/Views/Shared/_Layout.cshtml";
    <div class="container headerBreadcrumb">
        @Html.ActionLink("Home", "Dashboard", "SuperAdmin")
        <span class="easy-breadcrumb_segment-separator">/</span>
        @Html.ActionLink("Business Center Admin Portal", "Dashboard", "SuperAdmin")
        <span class="easy-breadcrumb_segment-separator">/</span>
        @Html.ActionLink("Customer", "CustomerHome", "Admin")
        <span class="easy-breadcrumb_segment-separator">/</span>
        @Html.ActionLink("BBL Dashboard", "CustomerSubmissions", "BBL", new { userId = @Model.UserId }, null)
        <span class="easy-breadcrumb_segment-separator">/</span>
        <span href="" class="easy-breadcrumb_segment easy-breadcrumb_segment-front">Receipt</span>
    </div>
    <div class="page_title">
        <div class="container">
            <div class="row">
                <h2 tabindex="0">View Receipt</h2>
            </div>
        </div>
    </div>

    <div class="wizard_form" id="printdiv">
        <div class="container">
            <div class="row">
            </div>
            <div class="collapse_panels" id="collapse_panels">
                <!-- expand/collapse section-->
                <div class="panel-group" id="checkListAccordion" role="tablist" aria-multiselectable="true">
                    <!-- panel 1-->
                    <div class="panel panel-default">
                        <div class="panel-heading row" role="tab" id="heading1">

                            <div class="panel-title">
                                <div class="checkListTostart section-space">
                                    @*<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 bot_border gradient-header" tabindex="0">*@
                                    <div class="col-md-offset-1 col-sm-offset-1 col-xs-offset-1 col-sm-6 col-md-6 col-xs-6 bot-space">
                                        <p>
                                            <span>Online Submission Number : </span>
                                            @Model.SubNumber
                                        <p>
                                        <p>
                                            <span>Amount Charged : </span>
                                            @string.Format(new CultureInfo("en-US"), "{0:C}", @Model.TotalFee)   <span class="small "><em>(all fees are nonrefundable)</em> </span>
                                        </p>
                                        <p>
                                            <span>Payment ID: </span>
                                            @Model.TransactionId
                                        </p>
                                    </div>
                                    <div class="col-sm-4 no-pad">
                                        @if (Model.CardNumber != "")
                                        {
                                            <p>
                                                <span>Receipt Date : </span>
                                                @Model.ReceiptDate
                                            </p>

                                        }
                                        @if (!string.IsNullOrEmpty(Model.CardNumber))
                                        {
                                            <p>
                                                <span>Credit Card : </span>
                                                @Model.CardNumber
                                            </p>

                                        }
                                    </div>
                                    @*</div>*@
                                </div>
                            </div>
                        </div>

                        <div style="border-top: 2px solid #124f87; margin-bottom: 10px;">

                            @*@if (Model.IsEhopAllowed)
                                {

                                    <ul style="list-style:none">
                                        <li style="color: #414141; font-family: 'Open Sans', sans-serif; font-size: 14px; font-weight: 400; line-height: 25px; padding-top: 10px;">
                                            <strong>Note: </strong>A copy of your Expedited Home Occupancy Permit (EHOP) may be downloaded from your My BBLs page.
                                            Please select the link to <a href="https://mybusiness.dc.gov/" style="color: blue"> mybusiness.dc.gov</a>
                                            and select My BBLs when you arrive on your Dashboard Page. You may need to log into the site if you are not already logged in.
                                        </li>
                                    </ul>
                                }*@
                            @if (Model.IsEhopAllowed && Model.ApplicationStatus == "UnderReview")
                            {

                                <p style="font-size: 15px; line-height: 22px; margin: 10px 10px 0 10px; font-weight: 500;font-family: 'Open Sans', sans-serif;">
                                    <strong>
                                        Note:
                                    </strong> A copy of your Expedited Home Occupancy Permit (EHOP) may be downloaded from your My BBLs page.
                                    Please select the link to <a href="https://mybusiness.dc.gov/" style="color: blue">mybusiness.dc.gov</a>
                                    and select My BBLs when you arrive on your Dashboard Page. You may need to log into the site if you are not already logged in.
                                </p>

                            }

                            @if (Model.IsEhopAllowed && (Model.ApplicationStatus == "Active" || Model.ApplicationStatus == "ACTIVE"))
                            {

                                <p style="font-size: 15px; line-height: 22px; margin: 10px 10px 0 10px; font-weight: 500;font-family: 'Open Sans', sans-serif;">

                                    <strong>Note:</strong> A copy of your Expedited Home Occupancy Permit (EHOP) and Business License may be downloaded from your My BBLs page. Please select the link to <a href="https://mybusiness.dc.gov/" style="color: blue">mybusiness.dc.gov</a> and select My BBLs when you arrive on your Dashboard Page. You may need to log into the site if you are not already logged in.
                                </p>

                            }

                            @if (!Model.IsEhopAllowed && (Model.ApplicationStatus == "Active" || Model.ApplicationStatus == "ACTIVE"))
                            {

                                <p style="font-size: 15px; line-height: 22px; margin:10px 10px 0 10px; font-weight: 500;font-family: 'Open Sans', sans-serif;">
                                    <strong>Note:</strong> A copy of your Business License may be downloaded from your My BBLs page. Please select the link to <a href="https://mybusiness.dc.gov/" style="color: blue">mybusiness.dc.gov</a> and select My BBLs when you arrive on your Dashboard Page. You may need to log into the site if you are not already logged in.
                                </p>

                            }
                            @if (Model.DocType != "" && Model.DocType == "IN")
                            {

                                <p style="font-size: 17px; line-height: 22px; font-weight: 500;">
                                    You elected to submit required supporting documents in person at the DCRA Service Center.
                                    You must submit those documents within 30 Calendar Days
                                    of this online submission. Failure to submit these documents by
                                    <strong style="color: red;"> @Model.ExceptedFinalCheckingDate</strong> may result in your application being
                                    rejected.
                                </p>

                            }

                            <div class="panel-body panel-collapse checklist-item">
                                @if ((Model.DocType != "IN") && (Model.DocType != ""))
                                {
                                    <p style="font-size: 17px; line-height: 22px; font-weight: 500;">
                                        The DCRA Business License Administration will review your application and all supporting documents submitted online.
                                        <span class="allow_business_note" style="color: red; font-family: 'Open Sans', sans-serif; font-size: 15px; font-weight: bold;">
                                            Please allow one business day for the processing of your application to begin.
                                        </span> The review will be completed within
                                        <strong>five (5) Business Days.</strong>
                                    </p>
                                    <p>Please refer to your My BBLs page to track the status of your application and to access a printable copy of your license once it is approved.</p>
                                    <p>
                                        Individual Licenses will not display a PDF copy on the My BBLs Page.
                                    </p>
                                    if (Model.IsBackgroundInvestigation == true)
                                    {
                                        <p>
                                            One&nbsp;of&nbsp;the license&nbsp;categories&nbsp;you&nbsp;have&nbsp;selected requires review by the Regulatory Investigation Section&nbsp;(RIS)&nbsp;which may add up to 30 days to your license review.<a href="#" class="descrlink">
                                                <img src="~/images/help_question_icon.png" style="height:22px;width:22px" class="cursor_type" ng-click="vm.showdescription()" />
                                            </a>
                                        </p>
                                    }
                                    <ul style="list-style: none">
                                        <li style="color: #414141; font-family: 'Open Sans', sans-serif; font-size: 14px; font-weight: 400; line-height: 25px;">
                                            @if (Model.SubmitTypeFrom.ToUpper() != "RENEWAL")
                                            {
                                                <a href="" style="cursor: default; pointer-events: none">Successful Application:</a>
                                            }
                                            else
                                            {
                                                <a href="" style="cursor: default; pointer-events: none">Successful Renewal:</a>
                                            }

                                            If all requested information and documents are approved, you will receive your license via US Mail within
                                            <strong><span style="color: red">5 to 7</span> Business Days</strong> of the completion of the Review.
                                        </li>
                                        @if (Model.SubmitTypeFrom.ToUpper() != "RENEWAL")
                                        {
                                            <li style="color: #414141; font-family: 'Open Sans', sans-serif; font-size: 14px; font-weight: 400; line-height: 25px; padding-top: 10px;">
                                                <a href="" style="cursor: default; pointer-events: none">
                                                    Incomplete Application:
                                                </a> If any additional information is required, you will receive an email at the email address listed above in this
                                                receipt indicating what information needs to be provided. You will have <strong>30 Calendar Days</strong> to provide the additional information.
                                                If the additional information is not provided, your application may be rejected at that point.
                                            </li>
                                        }
                                        else
                                        {

                                            <li style="color: #414141; font-family: 'Open Sans', sans-serif; font-size: 14px; font-weight: 400; line-height: 25px; padding-top: 10px;">
                                                <a href="" style="cursor: default; pointer-events: none">
                                                    Incomplete Renewal:
                                                </a> If any additional information is required, you will receive an email at the email address listed above in this receipt indicating what information needs to be provided. You will have <strong> 30 Calendar Days </strong> to provide the additional information. If the additional information is not provided, your application may be rejected at that point.
                                            </li>
                                        }
                                        <li style="color: #414141; font-family: 'Open Sans', sans-serif; font-size: 14px; font-weight: 400; line-height: 25px; padding-top: 10px;">
                                            @if (Model.SubmitTypeFrom.ToUpper() != "RENEWAL")
                                            {
                                                <a href="" style="cursor: default; pointer-events: none">
                                                    Rejected Application:
                                                </a>
                                            }
                                            else
                                            {
                                                <a href="" style="cursor: default; pointer-events: none">
                                                    Rejected Renewal:
                                                </a>
                                            }
                                            If your application is rejected, you will receive an email at the email address listed above in this receipt
                                            explaining why your application was insufficient and how you may appeal the agency's decision.
                                        </li>

                                        @*<li style="color:red; font-family: 'Open Sans', sans-serif; font-size: 14px; font-weight: 400; line-height: 25px; padding-top: 10px;">
                                                <span class="allow_business_note">
                                                    Please allow one business day for the processing of your application to begin.
                                                </span>
                                            </li>*@
                                    </ul>
                                }
                                else
                                {
                                    if (@Model.SubmitTypeFrom.ToUpper() == "RENEWAL")
                                    {
                                        <p>Please refer to your My BBLs page to track the status of your application and to access a printable copy of your license once it is approved.</p>

                                    }
                                    <p>
                                        Individual Licenses will not display a PDF copy on the My BBLs Page.
                                    </p>

                                    if (Model.IsBackgroundInvestigation == true)
                                    {
                                        <p>
                                            One of the license categories you have selected requires review by the Regulatory Investigation Section (RIS) which may add up to 30 days to your license review.<a href="#" class="descrlink"><img src="~/images/help_question_icon.png" style="height:22px;width:22px" class="cursor_type" ng-click="vm.showdescription()" /></a>
                                        </p>
                                    }
                                }
                                <ul style="list-style:none">
                                    <li style="color: #414141; font-family: 'Open Sans', sans-serif; font-size: 14px; font-weight: 400; line-height: 25px; padding-top: 10px;">
                                        <span>
                                            <strong>Note: </strong>    Reference the Online Submission Number to identify your application.
                                        </span>
                                    </li>
                                </ul>
                            </div>
                            @*</div>*@

                            <p style="border-bottom: 2px solid #124f87; margin-left: 1%; margin-right: 1%;text-align: center;width: 98%;"></p>
                        </div>

                        <div id="collapse1" class="panel-collapse open in" aria-labelledby="heading1"
                             style="overflow: hidden; display: block;">

                            <div class="business-activities-wrapper">
                                <div class="row">

                                    <div class="col-md-15 col-sm-3 col-xl-3 col-xs-12 col-md-offset-5">
                                        <div class="panel  text-center grand_total">
                                            <div class="panel-body">

                                                @*<h1>@string.Format(new CultureInfo("en-US"), "{0:C}", Model.TotalFee)</h1>*@
                                                <h1 style="text-align: center; margin-top: 4px;" id="grandamount">$ @Model.GrandTotals</h1>
                                            </div>
                                            <div class="panel-footer">
                                                <span>
                                                    Grand<br /> Total
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                @if (Model.SubmitTypeFrom.ToUpper() == "RENEWAL")
                                {

                                    if (Model.ExtraAmount.ToString().Contains("500"))
                                    {
                                        <div class="center_div text-center topBorder" style="margin-bottom: 10px;">
                                            <span>
                                                <i>
                                                    As your License status is Expired, late fee of $500 is added.
                                                </i>
                                            </span>
                                        </div>
                                    }

                                    if (Model.ExtraAmount.ToString().Contains("250"))
                                    {
                                        <div class="center_div text-center topBorder" style="margin-bottom: 10px;">
                                            <span>
                                                <i>
                                                    As your License status is Lapsed, late fee of $250 is added.
                                                </i>
                                            </span>
                                        </div>
                                    }

                                }
                                <div style="clear: both;"></div>
                                <div class="row">
                                    <div class="panel-body">
                                        <div class="search_result adjust_font">
                                            <div>
                                                <section class="clearfix">

                                                    @foreach (var item in Model.ServiceCheckList)
                                                    {
                                                        <div class="preappgrid container">

                                                            <div class="innerRow">
                                                                <div class="col-sm-1 arrow no-pad hidden-xs">
                                                                    <span class="arrow-right see-details"></span>
                                                                </div>
                                                                <div class="col-sm-3 col-xs-12 no-pad bbl">
                                                                    <p>
                                                                        <span>Endorsement </span>
                                                                    </p>
                                                                    <div>

                                                                        <p style="font-size:15px;">
                                                                            @item.Endorsement
                                                                        </p>
                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-4 no-pad">
                                                                    <p>
                                                                        <span>Category ID : </span>
                                                                        @item.CategoryCode
                                                                    </p>
                                                                    <p>
                                                                        <span>License Category : </span>
                                                                        @item.LicenseCategory
                                                                    </p>
                                                                    @if (item.SubCategoryName != "")
                                                                    {
                                                                        <p><span>Sub-Category : </span> @item.SubCategoryName </p>
                                                                    }
                                                                    <!--<p><span> {{feeDetailsItem.endorsementTitle}} : </span> {{feeDetailsItem.endorsement}}</p>-->
                                                                </div>
                                                                <div class="col-sm-2 no-pad">
                                                                    <p>
                                                                        <span>Units :   </span>

                                                                        @item.Units
                                                                    <p>
                                                                    <p>
                                                                        <span>License Duration : </span>
                                                                        @if (item.LicenseDuration.ToUpper().Trim() == "FOUR (4) YEAR")
                                                                        {

                                                                            <span>4 year</span>
                                                                        }
                                                                        else
                                                                        {
                                                                            <span>2 year</span>
                                                                        }
                                                                    </p>
                                                                    <p>
                                                                        <span>Total Fee : </span>
                                                                        <strong>@string.Format(new CultureInfo("en-US"), "{0:C}", item.TotalFee)   </strong>
                                                                    </p>
                                                                </div>
                                                            </div>
                                                            <div class="details no-pad" style="margin-top: 20px;">
                                                                <div class="topBorder col-md-offset-1"></div>
                                                                <div class="col-md-offset-2">
                                                                    <div class="col-sm-10 no-pad text-center">
                                                                        <p>

                                                                            <span>Application Fee :</span>@string.Format(new CultureInfo("en-US"), "{0:C}", item.ApplicationFee)
                                                                        </p>
                                                                        <p>
                                                                            <span>Endorsement Fee :</span>@string.Format(new CultureInfo("en-US"), "{0:C}", item.EndorsementFee)
                                                                        </p>
                                                                        <p>
                                                                            <span>Category License Fee  :</span>@string.Format(new CultureInfo("en-US"), "{0:C}", item.CategoryLicenseFee)
                                                                        </p>
                                                                    </div>
                                                                    <div class="col-sm-10 no-pad text-center">

                                                                        <div class="topBorder col-md-offset-2"></div>
                                                                        <p>
                                                                            <span>Sub Total :</span>@string.Format(new CultureInfo("en-US"), "{0:C}", item.SubTotal)
                                                                        </p>
                                                                        <p class="hidden-sm hidden-xs">
                                                                            <span>10% Tech Fee  :</span>@string.Format(new CultureInfo("en-US"), "{0:C}", item.TechFee)
                                                                        </p>
                                                                        <p class="visible-sm visible-xs">
                                                                            <span>10% Tech Fee  :</span> @string.Format(new CultureInfo("en-US"), "{0:C}", item.TechFee)
                                                                        </p>
                                                                        @if (item.LapsedFee != 0 && item.ExpiryFee == 0)
                                                                        {
                                                                            <p>
                                                                                <span>Late Fee : </span>$250.00
                                                                            </p>

                                                                        }
                                                                        @if (item.ExpiryFee != 0 && item.LapsedFee != 0)
                                                                        {
                                                                            <p>
                                                                                <span> Late Fee : </span> $500.00
                                                                            </p>

                                                                        }
                                                                        @if (item.LicenseCategory == "Apartment" || item.LicenseCategory == "One Family Rental")
                                                                        {

                                                                            <p style="font-size: 14px;">
                                                                                <strong> Category License Fee includes an RAO fee of $43.00/unit.</strong>
                                                                            </p>

                                                                        }
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    }
                                                </section>
                                                @if (Model.Isehop)
                                                {
                                                    <div class="col-sm-12 text-center no-pad">
                                                        <p style="font-size: 14px;">
                                                            <strong>
                                                                ** An eHOP fee of $72.60 ($66.00 (eHOP fee)+ 10% Technology Fee)
                                                                was added to the Grand Total displayed above.
                                                            </strong>
                                                        </p>
                                                    </div>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    @if (Model.DocType == "IN")
                    {
                        <div class="panel panel-default" id="inperson">
                            <div class="panel-heading row" role="tab" id="heading3">
                                <div class="panel-title">
                                    <div class="checkListTostart">
                                        <div class="col-lg-8 col-md-7 col-sm-6 col-xs-12 bot_border" tabindex="0">
                                            <h3>Supporting Documents </h3>
                                        </div>
                                        <div class="col-lg-4 col-md-5 col-sm-6 col-xs-12 btn_cont bot_border">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div id="collapse3" class="panel-collapse collapse in" aria-labelledby="heading3">
                                <div class="panel-body" tabindex="-1">
                                    @if (Model.DocumentList.Count() == 0)
                                    {
                                        <section id="noResultsKey">

                                            <div class="container1">
                                                <div class="innerRow">
                                                    <div class="col-md-12 col-xs-12 col-sm-12 text-center">
                                                        <p class="text-danger error_text ">No Supporting Documents Required</p>
                                                    </div>
                                                </div>
                                            </div>
                                        </section>
                                    }
                                    else
                                    {

                                        foreach (var item in Model.DocumentList.Select((x, i) => new { Data = x, Index = i }))
                                        {
                                            <section>
                                                <div class="container1">
                                                    <div class="innerRow">
                                                        <div class="col-sm-1 no-pad">
                                                            <span>@(item.Index + 1)</span>
                                                        </div>
                                                        <div class="col-sm-3 col-xs-12 no-pad bbl">
                                                            <p>
                                                                <span>Document Required</span>
                                                            </p>
                                                            <div>
                                                                @item.Data.DocRequired
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-1 col-xs-12"></div>
                                                        <div class="col-sm-4 col-xs-12 no-pad ">
                                                            <p>
                                                                <span>Agency : </span>
                                                                @item.Data.Agency
                                                            </p>
                                                            <p>
                                                                <span> Division : </span>@item.Data.Division
                                                            </p>
                                                        </div>
                                                        <div class="col-sm-3 col-xs-12 no-pad">
                                                            <p>
                                                                <span>License Code : </span>
                                                                @item.Data.CategoryCode
                                                            </p>
                                                            <p>
                                                                <span> License : </span>@item.Data.License
                                                            </p>
                                                        </div>

                                                        <div class="clearfix col-md-12 no-pad">
                                                            <div class="clearfix col-sm-12 col-md-2 no-pad"></div>
                                                            <div class="clearfix col-sm-12 col-md-11 no-pad col-lg-offset-1">
                                                                <p>
                                                                    @if (Model.DocType != "" && Model.DocType == "IN")
                                                                    {

                                                                    }
                                                                    else
                                                                    {
                                                                        if (item.Data.UploadFileName != "" && item.Data.UploadFileName != null)
                                                                        {
                                                                            <span>File Name :</span>
                                                                            @Html.ActionLink(@item.Data.UploadFileName, "Download", "BBL", new { fileId = @item.Data.UploadFileName }, null)
                                                                        }

                                                                    }
                                                                </p>
                                                            </div>
                                                        </div>

                                                        <div class="col-sm-12 col-xs-12">
                                                            <div class="grid-row-line"></div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </section>
                                        }

                                    }
                                </div>
                            </div>
                        </div>
                    }

                    <div class="pre-checkquestion col-md-12 col-xs-12 text-center" id="buttons">
                        <button id="btn_app_review" class="btn btn-default">Return to BBL Dashboard</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <style>
    </style>
    <script>
        var text = $('#grandamount').text();
        var charsLength = text.length;

        if (charsLength > 9 && charsLength <= 12) {
            $(".grand_total").css("width", "130%");
        } else if (charsLength > 12) {
            $(".grand_total").css("width", "160%");
        }

        $("#btn_app_review").click(function () {

            var url = "/BBL/CustomerSubmissions?userId=" + '@Model.UserId';
            window.location.href = url;
        });
    </script>
    <script>
        $(".arrow-right").click(function (event) {
            if ($(event.target).hasClass('see-details')) {
                $(event.target).addClass('hide-details').removeClass('see-details');
                $(event.target).parent().parent().css('border-bottom', '0px solid');
                $(event.target).parent().parent().parent().find('.details').show();
            }
            else {
                $(event.target).addClass('see-details').removeClass('hide-details');
                $(event.target).parent().parent().parent().find('.details').hide();
            }
        });
    </script>

    <div class="modal fade" id="description" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog modal-lg" role="document" style="width:80%">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title text-center" style="font-weight: 600" id="myModalLabel">License Categories Requiring RIS Review</h4>
                </div>
                <div class="modal-body" style="max-height: 520px; overflow-y: hidden !important;">
                    <div class="col-md-12">
                        <div class="col-md-6">
                            <h4 style="margin: 0.8em !important;">Auction Sale</h4>
                            <h4 style="margin: 0.8em !important;">Auctioneer</h4>
                            <h4 style="margin: 0.8em !important;">Automobile Repossessor</h4>
                            <h4 style="margin: 0.8em !important;"> Automobile Repossessor - Business</h4>
                            <h4 style="margin: 0.8em !important;">Consumer Goods (Auto Repair)</h4>
                            <h4 style="margin: 0.8em !important;">Consumer Goods (Electronic Repair)</h4>
                            <h4 style="margin: 0.8em !important;">Gen Contr/Construction Mngr</h4>
                            <h4 style="margin: 0.8em !important;">Home Improvement Contractor</h4>
